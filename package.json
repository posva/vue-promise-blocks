  "repository": {
    "type": "git",
    "url": "git+https://github.com/posva/vue-promised.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "codecov": "^3.3.0",
    "eslint": "^6.0.1",
    "eslint-config-posva": "^3.0.1",
    "faked-promise": "^2.1.0",
    "jest": "^24.7.1",
    "typescript": "^3.4.3",
