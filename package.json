    "dev": "yarn run unit -- --watchAll",
    "types": "tsc -p ./types/test/tsconfig.json",
    "pretest": "yarn run lint",
    "test": "yarn run types && yarn run unit",
    "prepublishOnly": "rollit"
  },
  "files": [
    "src",
    "types/*.d.ts",
    "dist/*.js",
    "LICENSE",
    "README.md"
